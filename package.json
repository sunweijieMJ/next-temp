{
  "name": "next-temp",
  "version": "1.0.0",
  "description": "next website",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "cz": "git-cz",
    "format": "prettier --write .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "npm run lint:script && npm run lint:style && npm run lint:cspell",
    "lint:fix": "npm run lint:script && npm run lint:style && npm run lint:cspell && pretty-quick --check",
    "lint:cspell": "cspell **",
    "lint:script": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:style": "stylelint 'src/**/*.{css,scss,less}' --fix",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*": [
      "npm run lint:cspell"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:script"
    ],
    "*.{css,scss,less}": [
      "npm run lint:style"
    ]
  },
  "dependencies": {
    "@redux-devtools/extension": "^3.2.3",
    "ahooks": "^3.7.2",
    "antd": "^5.0.3",
    "axios": "^1.2.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.6",
    "lodash": "^4.17.21",
    "next": "13.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^6.2.5",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.11.65",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "commitizen": "^4.2.5",
    "cspell": "^6.16.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.12.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.9.3"
  }
}
